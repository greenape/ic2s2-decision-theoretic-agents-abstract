keywords = {Adaptation, Physiological,Animals,Computer Simulation,Hemodynamics,Mice,Mice, Nude,Microvessels,Microvessels: physiology,Microvessels: physiopathology,Models, Cardiovascular,Neoplasm Transplantation,Neoplasms,Neoplasms: blood supply,Neoplasms: physiopathology,Neovascularization, Pathologic,Oxygen,Oxygen: metabolism,Splanchnic Circulation,Splanchnic Circulation: physiology},
number = {5},
pages = {e1000394},
