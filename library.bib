file = {::},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Algorithms,Animals,Artificial Intelligence,Computer Simulation,Drosophila melanogaster,Drosophila melanogaster: genetics,Drosophila melanogaster: growth \& development,Federal Government,Gene Regulatory Networks,Models, Biological,Publishing,Regression Analysis,Social Support,Time Factors,United States},
