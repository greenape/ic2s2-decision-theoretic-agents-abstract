keywords = {Adult,Attitude of Health Personnel,Community Health Nursing,Community Health Nursing: organization \& administr,Continuity of Patient Care,Continuity of Patient Care: standards,Female,Humans,Interprofessional Relations,Job Satisfaction,Maternal Health Services,Maternal Health Services: organization \& administr,Middle Aged,Morale,Nurse Midwives,Nurse Midwives: organization \& administration,Nurse Midwives: psychology,Nursing Staff, Hospital,Nursing Staff, Hospital: organization \& administra,Nursing Staff, Hospital: psychology,Nursing, Team,Nursing, Team: organization \& administration,Outcome and Process Assessment (Health Care),Questionnaires,Workload},
