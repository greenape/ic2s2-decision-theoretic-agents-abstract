@article{Berk2013,
abstract = {BACKGROUND: We now know that depression is associated with a chronic, low-grade inflammatory response and activation of cell-mediated immunity, as well as activation of the compensatory anti-inflammatory reflex system. It is similarly accompanied by increased oxidative and nitrosative stress (O\&NS), which contribute to neuroprogression in the disorder. The obvious question this poses is 'what is the source of this chronic low-grade inflammation?'

DISCUSSION: This review explores the role of inflammation and oxidative and nitrosative stress as possible mediators of known environmental risk factors in depression, and discusses potential implications of these findings. A range of factors appear to increase the risk for the development of depression, and seem to be associated with systemic inflammation; these include psychosocial stressors, poor diet, physical inactivity, obesity, smoking, altered gut permeability, atopy, dental cares, sleep and vitamin D deficiency.

SUMMARY: The identification of known sources of inflammation provides support for inflammation as a mediating pathway to both risk and neuroprogression in depression. Critically, most of these factors are plastic, and potentially amenable to therapeutic and preventative interventions. Most, but not all, of the above mentioned sources of inflammation may play a role in other psychiatric disorders, such as bipolar disorder, schizophrenia, autism and post-traumatic stress disorder.},
author = {Berk, Michael and Williams, Lana J and Jacka, Felice N and O'Neil, Adrienne and Pasco, Julie a and Moylan, Steven and Allen, Nicholas B and Stuart, Amanda L and Hayley, Amie C and Byrne, Michelle L and Maes, Michael},
file = {:Users/jono/Documents/Mendeley Desktop/Berk et al/BMC medicine/Berk et al. - 2013 - So depression is an inflammatory disease, but where does the inflammation come from.pdf:pdf},
journal = {BMC medicine},
keywords = {atopic,cytokines,dental cares,depression,diet,exercise,gut,inflammation,obesity,oxidative stress,sleep,smoking,vitamin d},
number = {1},
pages = {200},
pmid = {24228900},
publisher = {BMC Medicine},
title = {{So depression is an inflammatory disease, but where does the inflammation come from?}},
volume = {11},
year = {2013}
}
