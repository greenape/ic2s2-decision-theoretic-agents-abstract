journal = {Alcoholism: Clinical \& Experimental Research},
keywords = {Adult,Alcohol Drinking,Alcohol Drinking: epidemiology,Data Collection,Depression,Depression: epidemiology,Depression: psychology,Female,Humans,Longitudinal Studies,Marriage,Mental Health,Personal Satisfaction,Population,Predictive Value of Tests,Pregnancy,Pregnancy: statistics \& numerical data,Questionnaires,Retrospective Studies,Sweden,Sweden: epidemiology},
