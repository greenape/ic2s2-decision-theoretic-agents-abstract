file = {:Users/jono/Documents/Mendeley Desktop/Bechara, Damasio, Damasio/Cerebral cortex (New York, N.Y. 1991)/Bechara, Damasio, Damasio - 2000 - Emotion, decision making and the orbitofrontal cortex.pdf:pdf},
journal = {Cerebral cortex (New York, N.Y. : 1991)},
keywords = {Animals,Cognition,Cognition: physiology,Decision Making,Decision Making: physiology,Emotions,Emotions: physiology,Frontal Lobe,Frontal Lobe: physiology,Humans},
