keywords = {Continuity of Patient Care,Depression,Female,Humans,London,London: epidemiology,Mental Disorders,Mental Disorders: epidemiology,Midwifery,Patient Dropouts,Postpartum,Postpartum: epidemiology,Postpartum: prevention \& control,Pregnancy,Risk Factors},
number = {2},
pages = {119--27},
pmid = {12735413},
title = {{Can we prevent postnatal depression? A randomized controlled trial to assess the effect of continuity of midwifery care on rates of postnatal depression in high-risk women.}},
volume = {13},
year = {2003}
}
